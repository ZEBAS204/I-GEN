{
  "name": "react-anc",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "lint": "stylelint ./src/**/*.scss || echo \"Lint failed for some file(s).\"",
    "start": "react-scripts start",
    "dev": "set BROWSER=NONE && npm run start",
    "dev:build": "npm run build && serve -s build -l 5000",
    "dev:serve": "serve -s build -l 5000",
    "dev-webpack": "webpack-dev-server --mode development",
    "build": "react-scripts build && gulp",
    "bundle-build": "webpack --mode production",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.6.2",
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@reduxjs/toolkit": "^1.6.0",
    "framer-motion": "^4.1.17",
    "i18next": "^20.3.2",
    "i18next-browser-languagedetector": "^6.1.1",
    "i18next-http-backend": "^1.2.6",
    "localforage": "^1.9.0",
    "node-sass": "^5.0.0",
    "normalize.css": "^8.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hotkeys-hook": "^3.4.3",
    "react-i18next": "^11.8.15",
    "react-icons": "^4.2.0",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "5.0.1",
    "react-toolbox": "^2.0.0-beta.13",
    "workbox-background-sync": "^5.1.3",
    "workbox-broadcast-update": "^6.1.5",
    "workbox-cacheable-response": "^6.1.5",
    "workbox-core": "^6.1.5",
    "workbox-expiration": "^6.1.5",
    "workbox-navigation-preload": "^5.1.3",
    "workbox-precaching": "^5.1.3",
    "workbox-range-requests": "^6.1.5",
    "workbox-routing": "^5.1.3",
    "workbox-strategies": "^6.1.5",
    "workbox-streams": "^6.1.5"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gulp": "^4.0.2",
    "serve": "^12.0.0",
    "source-map-explorer": "^2.5.2",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.21.0",
    "webpack-cli": "^4.7.2"
  }
}
