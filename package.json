{
  "name": "react-anc",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "lint": "stylelint ./src/**/*.scss || echo \"Lint failed for some file(s).\"",
    "start": "react-scripts start",
    "dev": "set BROWSER=NONE && set PORT=3000 && npm run start",
    "dev:build": "npm run build && npm run dev:serve",
    "dev:serve": "serve -s build -l 5000",
    "dev-webpack": "webpack-dev-server --mode development",
    "build": "react-scripts build",
    "bundle-build": "webpack --mode production",
    "test": "react-scripts test",
    "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.8.6",
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "framer-motion": "^4.1.17",
    "i18next": "^20.3.2",
    "i18next-browser-languagedetector": "^6.1.3",
    "i18next-http-backend": "^1.4.0",
    "localforage": "^1.9.0",
    "loglevel": "^1.8.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.16.1",
    "react-icons": "^4.2.0",
    "react-modal-sheet": "^1.5.1",
    "react-router-dom": "^6.2.2",
    "react-scripts": "4.0.3",
    "react-use": "^17.3.2",
    "sass-mq": "^6.0.0",
    "workbox-background-sync": "^5.1.3",
    "workbox-broadcast-update": "^6.5.1",
    "workbox-cacheable-response": "^6.5.1",
    "workbox-core": "^6.5.1",
    "workbox-expiration": "^6.5.1",
    "workbox-navigation-preload": "^5.1.3",
    "workbox-precaching": "^5.1.3",
    "workbox-range-requests": "^6.5.1",
    "workbox-routing": "^5.1.3",
    "workbox-strategies": "^6.5.1",
    "workbox-streams": "^6.5.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "sass": "^1.49.9",
    "serve": "^12.0.0",
    "source-map-explorer": "^2.5.2",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.21.0",
    "webpack-cli": "^4.9.2"
  }
}
